import java.util.Scanner;
public class Sum
{
  public static void main(String[] args)
  {
    int a,b,sum=0;
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter the values of aand b:");
    a=sc.nextInt();
    b=sc.nextInt();
    sum=a+b;
    System.out.print("sum=%d"+sum);
    }
 }
    
